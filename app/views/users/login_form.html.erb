<h1>Users#login</h1>
<p>Find me in app/views/users/login.html.erb</p>

<%= @message %>


<%= form_tag("/login", method: "post") do %>

  <%= label_tag(:username, "Username:") %>
  <%= text_field_tag(:username) %><br>
  <%= label_tag(:password, "Password:") %>
  <%= password_field_tag(:password) %>

  <%= submit_tag("Submit") %>
<% end %>
